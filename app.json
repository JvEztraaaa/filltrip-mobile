{
  "expo": {
    "name": "filltrip-mobile",
    "slug": "filltrip-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "filltripmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "MGLMapboxAccessToken": "$(EXPO_PUBLIC_MAPBOX_TOKEN)",
        "NSLocationWhenInUseUsageDescription": "This app uses location to show your position on the map and help with route planning."
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.filltrip.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      ["@rnmapbox/maps", { "RNMapboxMapsDownload": "static" }],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app uses location to show your position on the map and help with route planning."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a27c484c-b965-48ec-a375-5655dbaecd91"
      },
      "expoClient": {
        "mapboxToken": "$(EXPO_PUBLIC_MAPBOX_TOKEN)"
      }
    }
  }
}
