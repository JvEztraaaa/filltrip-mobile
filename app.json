{
  "expo": {
    "name": "filltrip-mobile",
    "slug": "filltrip-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "scheme": "filltripmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      // Remove adaptiveIcon images for now; Expo Go will use defaults
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    }
  }
}
